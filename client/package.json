    "@hookform/resolvers": "^2.7.0",
    "axios": "^0.21.1",
    "date-fns": "^2.23.0",
    "framer-motion": "^4.1.17",
    "fuse.js": "^6.4.6",
    "passport": "^0.4.1",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.10.1",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.4",
