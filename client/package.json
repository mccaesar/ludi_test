{
  "name": "ludi-client",
  "homepage": "https://luditesttest.web.illinois.edu/",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "clean": "rm -rf dist && mkdir dist",
    "build": "yarn clean && yarn build:prod && cp .htaccess ./dist && zip -rj ./dist/bundle.zip ./dist",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:dev": "yarn clean && webpack --mode=development",
    "build:prod": "yarn clean && webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.14",
    "@chakra-ui/react": "^1.6.5",
    "@emotion/react": "^11.4.0",
